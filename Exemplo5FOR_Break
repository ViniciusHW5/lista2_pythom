pessoas_presentes = ['John Snow','Jesse Pinkman','Aria Stark','Tyrion Lannister']
#quero Saber se uma pessoas especifica está presente.
chamada = 'Aria Stark'
for pessoa in pessoas_presentes:
    if pessoa == chamada:
        print ('{} está presente .'.format(chamada))
        break
    else:
        print('só um print para mostrar que o for passou por essas pessoa:'+str(pessoa))#continue

    #print
    
